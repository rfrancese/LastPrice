<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.lp_lastprice.DettagliActivity"
    tools:ignore="MergeRootFrame" >

    <ImageButton
        android:id="@+id/Call"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="250dp"
        android:layout_marginLeft="20dp"
        android:src="@drawable/phone_button" />

    <ImageButton
        android:id="@+id/Maps"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
         android:layout_marginTop="250dp"
        android:layout_marginLeft="200dp"
        android:src="@drawable/maps_icon"
        android:onClick="openMaps" />
 <TextView
        android:id="@+id/TextView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Descrizione"
       />
    <TextView
        android:id="@+id/Description"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Medium Text"
        android:layout_marginTop="20dp"
      
        android:textAppearance="?android:attr/textAppearanceMedium" />
 <TextView
        android:id="@+id/TextView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="50dp"
        android:text="Prezzo"
       />
    <TextView
        android:id="@+id/Prezzo_form"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Medium Text"
          android:layout_marginTop="70dp"
        android:textAppearance="?android:attr/textAppearanceMedium" />
     <TextView
        android:id="@+id/TextView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Luogo"
        android:layout_marginTop="100dp"
       />
    <TextView
        android:id="@+id/Luogo_form"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Medium Text"
          android:layout_marginTop="120dp"
        android:textAppearance="?android:attr/textAppearanceMedium" />
     <TextView
        android:id="@+id/TextView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Seller"
        android:layout_marginTop="150dp"
       />
    <TextView
        android:id="@+id/Seller_form"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Medium Text"
        android:layout_marginTop="170dp"
        android:textAppearance="?android:attr/textAppearanceMedium" />
    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Chiama"
          android:layout_marginTop="220dp"
          android:layout_marginLeft="20dp"
        android:textAppearance="?android:attr/textAppearanceMedium" />
    
  <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Mappa"
          android:layout_marginTop="220dp"
          android:layout_marginLeft="210dp"
        android:textAppearance="?android:attr/textAppearanceMedium" />
     <Button
        android:id="@+id/Buy"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Acquista"
        android:layout_marginLeft="100dp"
        android:layout_marginTop="260dp"
        android:onClick="Buy" />

</FrameLayout>

